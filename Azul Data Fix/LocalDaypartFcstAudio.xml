    <DataType>number</DataType>
  </Field>
  <Var>
    <Name>sunriseVar</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1}]/sunrise</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast[fcst_valid_local="{FcstValidLocal}"]/sunrise</Path>
  </Var>
  <Var>
    <Name>sunsetVar</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1}]/sunset</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast[fcst_valid_local="{FcstValidLocal}"]/sunset</Path>
  </Var>
  <Var>
    <Name>moonriseVar</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1}]/sunrise</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast[fcst_valid_local="{FcstValidLocal}"]/moonrise</Path>
  </Var>
  <Var>
    <Name>moonsetVar</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1}]/sunset</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast[fcst_valid_local="{FcstValidLocal}"]/moonset</Path>
  </Var>
  <PyField>
    <Name>sunrise</Name>
    <Script>FormatTime.py</Script>
    <ScriptParam>
      <Name>time</Name>
      <Value>{sunriseVar}</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>outputFmt</Name>
      <Value>%I:%M:%S %p</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>inputFmt</Name>
      <Value>%Y-%m-%dT%H:%M:%S-%z</Value>
    </ScriptParam>
    <EvaluateFunctionName>FormatTime()</EvaluateFunctionName>
  </PyField>
  <PyField>
    <Name>sunset</Name>
    <Script>FormatTime.py</Script>
    <ScriptParam>
      <Name>time</Name>
      <Value>{sunsetVar}</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>outputFmt</Name>
      <Value>%I:%M:%S %p</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>inputFmt</Name>
      <Value>%Y-%m-%dT%H:%M:%S-%z</Value>
    </ScriptParam>
    <EvaluateFunctionName>FormatTime()</EvaluateFunctionName>
  </PyField>
  <PyField>
    <Name>moonrise</Name>
    <Script>FormatTime.py</Script>
    <ScriptParam>
      <Name>time</Name>
      <Value>{moonriseVar}</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>outputFmt</Name>
      <Value>%I:%M:%S %p</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>inputFmt</Name>
      <Value>%Y-%m-%dT%H:%M:%S-%z</Value>
    </ScriptParam>
    <EvaluateFunctionName>FormatTime()</EvaluateFunctionName>
  </PyField>
  <PyField>
    <Name>moonset</Name>
    <Script>FormatTime.py</Script>
    <ScriptParam>
      <Name>time</Name>
      <Value>{moonsetVar}</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>outputFmt</Name>
      <Value>%I:%M:%S %p</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>inputFmt</Name>
      <Value>%Y-%m-%dT%H:%M:%S-%z</Value>
    </ScriptParam>
    <EvaluateFunctionName>FormatTime()</EvaluateFunctionName>
  </PyField>
  <Field>
    <Name>precipChance</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/pOP</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/pop</Path>
    <Description>Probability of precipation for the hour</Description>
    <DataType>number</DataType>
  </Field>
  <Field>
    <Name>precipType</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/precipTyp</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/precip_type</Path>
    <Description>precipTyp - precipitation type for hourly forecast</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>precipChanceAmt</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/qpf</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast[fcst_valid_local="{FcstValidLocal}"]/qpf</Path>
    <Description>Quantative precipatation forecast for 24 hours in inchesNotes - QPF is associated to the snsblWx and tSnsblWx fields.  Therecan be trace amounts of precipation resulting in 0 qpf while text phrasesdisplay precipation</Description>
    <DataType>number</DataType>
  </Field>
  <Field default="N/A">
    <Name>snowChance</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/cPOS</Path> -->
    <Path>LFRecord:{0}/LFData/coopId//*[text() = 'XXXXXX']/text()</Path>
    <Description>Conditional probability of snow for period.  Daily is 24 hours, Daypart 12 or 24 hours, hourly one hour.</Description>
    <DataType>number</DataType>
  </Field>
  <Field>
