    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/uvDes</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/uv_desc</Path>
    <Description>UV index description</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>uvIndexAmt</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/uvIdx</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/uv_index</Path>
    <Description>UV index</Description>
    <DataType>number</DataType>
  </Field>
  <Field>
    <Name>golfIndex</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/glfCat</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/golf_category</Path>
    <Description>Golf Category</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>golfIndexAmt</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/glfIdx</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/golf_index</Path>
