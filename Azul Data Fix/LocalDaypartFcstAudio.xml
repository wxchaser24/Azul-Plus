<DataViewDescriptor>
  <Key>
    <KeyPart>LocKey</KeyPart>
    <KeyPart>DateTimePart1</KeyPart>
    <KeyPart>FcstMapping</KeyPart>
    <KeyPart>WxManMapping</KeyPart>
  </Key>
  <PyVar>
    <Name>FcstDateTm</Name>
    <Script>FormatFcstDateTm.py</Script>
    <ScriptParam>
      <Name>datepart</Name>
      <Value>{1}</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>timepart</Name>
      <Value>{2}</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>outputFmt</Name>
      <Value>%Y-%m-%dT%H:%M:%S</Value>
    </ScriptParam>
    <ScriptParam>
      <Name>inputFmt</Name>
      <Value>%Y%m%d%H%M%S</Value>
    </ScriptParam>
    <EvaluateFunctionName>FormatFcstDateTm()</EvaluateFunctionName>
  </PyVar>
  <Var>
    <Name>CoopId</Name>
    <Path>LFRecord:{0}/LFData/coopId</Path>
  </Var>
  <CondVar>
    <Name>DayNightNode</Name>
    <Case>
      <Path>DailyForecast:{CoopId}/forecasts/forecast/day[starts-with(fcst_valid_local,"{FcstDateTm}")]/fcst_valid_local</Path>
      <Result>day</Result>
    </Case>
    <Case>
      <Path>DailyForecast:{CoopId}/forecasts/forecast/night[starts-with(fcst_valid_local,"{FcstDateTm}")]/fcst_valid_local</Path>
      <Result>night</Result>
    </Case>
    <Default>
      <Result>FcstDateTm_not_found</Result>
    </Default>
  </CondVar>
  <Var>
    <!-- fcst_valid_local is %Y-%m-%dT%H:%M:%S with a suffix of what appears to be numeric timezone (i.e. -0500) 
		 so we search for the prefix to get the exact value value
	-->
    <Name>FcstValidLocal</Name>
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[starts-with(fcst_valid_local,"{FcstDateTm}")]/fcst_valid_local</Path>
  </Var>
  <ConstField>
    <Name>FcstValidLocalValue</Name>
    <Value>{FcstValidLocal}</Value>
  </ConstField>
  <ConstField>
    <Name>ParentNodeValue</Name>
    <Value>{DayNightNode}</Value>
  </ConstField>
  <Field>
    <Name>locId</Name>
    <Path>LFRecord:{0}/LFHdr/locId</Path>
    <Description>TWC location identifier</Description>
    <DataType>string</DataType>
  </Field>
  <Field overridable="true">
    <Name>locName</Name>
    <Path>LFRecord:{0}/LFData/prsntNm</Path>
    <Description>City presentation name (mixed case)</Description>
    <DataType>string</DataType>
  </Field>
  <Field overridable="true">
    <Name>locName16</Name>
    <Path>LFRecord:{0}/LFData/wrlsPrsntNm</Path>
    <Description>City presentation name short</Description>
    <DataType>string</DataType>
  </Field>
  <Field overridable="true">
    <Name>locName26</Name>
    <Path>LFRecord:{0}/LFData/tPrsntNm</Path>
    <Description>City presentation name long</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>locLat</Name>
    <Path>LFRecord:{0}/LFData/lat</Path>
    <Description>Latitude</Description>
    <DataType>number</DataType>
  </Field>
  <Field>
    <Name>locLong</Name>
    <Path>LFRecord:{0}/LFData/long</Path>
    <Description>Longitude</Description>
    <DataType>number</DataType>
  </Field>
  <Field>
    <Name>locObsStn</Name>
    <Path>LFRecord:{0}/LFData/obsStn</Path>
    <Description>NWS Observation station</Description>
    <DataType>string</DataType>
  </Field>
  <Field default="N/A">
