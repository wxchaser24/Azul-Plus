    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>wxPhrasePart1</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/wxPhrasePart1</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/subphrase_pt1</Path>
    <Description>part 1 of 3 part short phrase for DFRecord for selected locValTm</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>wxPhrasePart2</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/wxPhrasePart2</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/subphrase_pt2</Path>
    <Description>part 2 of 3 part short phrase for DFRecord for selected locValTm</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>wxPhrasePart3</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/wxPhrasePart3</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/subphrase_pt3</Path>
    <Description>part 3 of 3 part short phrase for DFRecord for selected locValTm</Description>
    <DataType>string</DataType>
  </Field>
