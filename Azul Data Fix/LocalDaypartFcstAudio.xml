    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/extQulfr</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast[fcst_valid_local="{FcstValidLocal}"]/qualifier_code</Path>
    <Description>Severe weather forecast for 12 hours</Description>
    <DataType>string</DataType>
  </Field>
  <Field default="N/A">
