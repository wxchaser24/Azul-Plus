  </Field>
  <Field>
    <Name>uvWarning</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/uvWrn</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/uv_warning</Path>
    <Description>UV warning</Description>
    <DataType>number</DataType>
  </Field>
  <Field>
