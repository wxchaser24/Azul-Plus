    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/dow</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast[fcst_valid_local="{FcstValidLocal}"]/dow</Path>
    <Description>Day of the week in local apparent time</Description>
    <DataType format="dddd">dayweek</DataType>
  </Field>
