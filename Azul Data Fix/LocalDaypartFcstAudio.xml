    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/snwAccum</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast[fcst_valid_local="{FcstValidLocal}"]/snow_code</Path>
    <Description>Snow accumulation forecast for 12 hours - in inches</Description>
    <DataType>string</DataType>
  </Field>
