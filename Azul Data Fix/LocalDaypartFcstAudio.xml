    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/alt_daypart_name</Path>
    <Description> altDyPrtNm - alternate daypart name - contains regular daypart names and holidays - subject to change without notice</Description>
    <DataType>string</DataType>
  </Field>
  <Field default="N/A">
    <Name>forecastHours</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/dur</Path> -->
    <Path>LFRecord:{0}/LFData/coopId//*[text() = 'XXXXXX']/text()</Path>
    <Description>Duration - range of hours daypart forecast is valid for</Description>
    <DataType>number</DataType>
  </Field>
  <Field>
    <Name>forecast</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/shrtCst</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/shortcast</Path>
    <Description>Abbreviated forecast</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>forecastLong</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/phrase</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/narrative</Path>
    <Description>Daypart text</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>sensibleFcst</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/snsblWx</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/phrase_32char</Path>
    <Description>Sensible weather phrase for period.   Daily is 24 hours, Daypart 12 or 24 hours, hourly one hour</Description>
    <DataType>string</DataType>
  </Field>
