    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/rH</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/rh</Path>
