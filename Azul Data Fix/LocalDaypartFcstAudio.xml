    <!-- <Path>DerivedDDRecord:{CoopId}/DDHdr/procTm</Path> -->
    <Path>DailyForecast:{CoopId}/procTm</Path>
    <Description>PFMS process time in YYYYMMDDHH24MISS</Description>
    <DataType format="yyyyMMddHHmmss">datetime</DataType>
  </Field>
  <Field>
    <Name>stationName</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDHdr/stnNm</Path> -->
    <Path>DailyForecast:{CoopId}/clientKey</Path>
    <Description>Station Name</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>dayPart</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/dyPrtNm</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/daypart_name</Path>
    <Description>Day part name</Description>
    <DataType>string</DataType>
  </Field>
  <Field>
    <Name>dayPartLong</Name>
    <!-- <Path>DerivedDDRecord:{CoopId}/DDData[locValDay={1} and locValTm={2}]/longDyPrt</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/long_daypart_name</Path>
    <Description>Long day part name</Description>
