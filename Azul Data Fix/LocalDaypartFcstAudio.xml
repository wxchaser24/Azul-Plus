    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/wxMan</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/wxman</Path>
  </Var>
  <CondVar>
    <Name>DayNightIcon</Name>
    <Case>
      <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/day_ind[. = 'D']</Path>
      <Result>DayIcon</Result>
    </Case>
    <Case>
      <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/day_ind[. = 'N']</Path>
      <Result>NightIcon</Result>
    </Case>
    <Default>
      <Result>XXX</Result>
    </Default>
  </CondVar>
  <CondVar>
    <Name>DayOrNightVar</Name>
    <Case>
      <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/day_ind[. = 'D']</Path>
      <Result>D</Result>
