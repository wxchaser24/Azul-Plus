    <DataType>number</DataType>
  </Field>
  <Field default="N/A">
    <Name>windChill</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/wCF</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/wc</Path>
    <Description>Wind chill in fahrenheit - only use below 40 F - 24 hour value</Description>
    <DataType>number</DataType>
  </Field>
  <Field default="N/A">
    <Name>windDirection</Name>
    <!-- <Path>DerivedDFRecord:{CoopId}/DFData[locValDay={1} and locValTm={2}]/wDirAsc</Path> -->
    <Path>DailyForecast:{CoopId}/forecasts/forecast/{DayNightNode}[fcst_valid_local="{FcstValidLocal}"]/wdir_cardinal</Path>
    <Description>Wind direction in text (ascii)</Description>
    <DataType>string</DataType>
  </Field>
  <Field default="N/A">
